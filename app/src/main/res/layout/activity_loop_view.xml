<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <com.fec.view.common.topbar.TopBar
        android:id="@+id/topbar3"
        app:topbar_title_color="@color/white"
        app:topbar_bg_color="@color/colorPrimary"
        android:layout_width="match_parent"
        android:layout_height="48dp"/>

    <com.fec.view.common.AutoLoopView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="60dp"
        android:id="@+id/alv"
        android:background="@color/windowBg"
        app:dot_size="8dp"
        app:dotLayoutGravity="center"
        app:changeDuration="800"
        app:autoChangeDuration="3000"
        />


</FrameLayout>