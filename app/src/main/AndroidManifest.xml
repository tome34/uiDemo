        <activity android:name=".activity.MsgCodeActivity">
        </activity>
        <activity android:name=".activity.ZxingActivity">
        </activity>
        <activity android:name=".activity.CameraActivity">
        </activity>
        <activity android:name=".activity.NumberActivity">
        </activity>
        <activity android:name=".activity.LoopViewActivity"/>
        <activity android:name=".activity.ImageListActivity">
        </activity>
        <activity android:name=".activity.ButtonStyleActivity">
        </activity>
        <activity android:name=".activity.AmapAddressActivity">
        </activity>

        <!-- 高德地图key -->
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="b8e13acefa18ca461267f42b0cbd3208">
        </meta-data>

        <activity android:name=".activity.SearchAddressActivity">
        </activity>
        <activity android:name=".activity.HeaderScrollViewActivity">
        </activity>
        <activity android:name=".activity.EmojiActivity">
        </activity>
    </application>

</manifest>