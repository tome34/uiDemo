        <activity android:name=".activity.CustomdownTimeActivity">
        </activity>
        <activity android:name=".activity.MarqueeStyleActivity">
        </activity>
        <activity android:name=".activity.PhotoViewActivity">
        </activity>
        <activity android:name=".activity.PictureSelectorActivity">
        </activity>
        <activity android:name=".activity.PopupAutoActivity">
        </activity>
        <activity android:name=".activity.BadgeActivity">
        </activity>
        <activity android:name=".activity.SearchActivity"/>
        <activity android:name=".activity.TextAreaActivity"/>
        <activity android:name=".activity.TranslucentActivity"/>
        <activity android:name=".activity.ImgTranslucentActivity"/>
        <activity android:name=".activity.RefreshLayoutActivity">
