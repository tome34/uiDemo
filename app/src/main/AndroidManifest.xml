        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.RatingBarActivity">
        </activity>
        <activity android:name=".activity.EmptyActivity">
        </activity>
        <activity android:name=".activity.SwitchButtonActivity">
        </activity>
        <activity android:name=".fragment.QDTabSegmentScrollableModeActivity"/>
